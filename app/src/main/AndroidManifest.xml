<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.donottouch.twitterlogin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".backbone.AddObjectToApp"
        >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".P000Menu"></activity>


        <activity android:name=".P001UserTimeline"></activity>

        <activity android:name=".P002HomeTimeline"></activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="xxx" />   //change it your own key
    </application>


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>
